<%if flash[:my_errors]%>
<%flash[:my_errors].each do |error|%>
<%= error%>
<%end %>
<%end %>

<%= form_for @duck do |f|%>
<%= f.label "Name"%>
<%= f.text_field :name%> <br>
<%= f.label "Description"%>
<%= f.text_field :description %> <br>
<%= f.collection_select :student_id, Student.all, :id, :name %>
<%= f.submit%>
<%end %>